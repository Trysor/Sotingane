{
	"name": "sotingserver",
	"version": "1.0.0",
	"description": "Server backend for Sotingane",
	"main": "dist/api.js",
	"scripts": {
		"build": "tsc",
		"dev": "tsc-watch --onSuccess \"node --inspect-brk dist/out-tsc/API/src/app.js\"",
		"lint": "tslint -c tslint.json 'src/**/*.ts'",
		"test:cont": "tsc-watch --onSuccess \"cross-env NODE_ENV=test node dist/out-tsc/API/test/testbed.js\"",
		"test": "npm run build && cross-env NODE_ENV=test node dist/out-tsc/API/test/testbed.js",
		"test:debug": "npm run build && mocha --inspect-brk --delay --exit ./dist/out-tsc/API/**/*.test.js",
		"webpack": "webpack --config webpack.config.js --progress --colors"
	},
	"dependencies": {
		"ajv": "6.12.2",
		"bcrypt": "^5.0.0",
		"config": "3.3.1",
		"cookie-parser": "1.4.5",
		"express": "4.17.1",
		"express-fileupload": "1.1.6",
		"helmet": "3.23.0",
		"image-size": "0.8.3",
		"jsonwebtoken": "8.5.1",
		"method-override": "3.0.0",
		"mongoose": "5.9.18",
		"morgan": "1.10.0",
		"passport": "0.4.1",
		"passport-jwt": "4.0.0",
		"passport-local": "1.0.0",
		"sanitize-html": "1.26.0",
		"sharp": "^0.25.4",
		"source-map-support": "0.5.19",
		"ua-parser-js": "0.7.21",
		"uuid": "8.1.0",
		"validator": "13.1.1"
	},
	"devDependencies": {
		"@types/bcrypt": "3.0.0",
		"@types/chai": "4.2.11",
		"@types/config": "0.0.36",
		"@types/cookie-parser": "1.4.2",
		"@types/express": "4.17.6",
		"@types/express-fileupload": "^1.1.3",
		"@types/helmet": "0.0.47",
		"@types/jsonwebtoken": "8.5.0",
		"@types/method-override": "0.0.31",
		"@types/mocha": "7.0.2",
		"@types/mongoose": "5.7.24",
		"@types/morgan": "1.9.1",
		"@types/node": "14.0.13",
		"@types/passport": "1.0.3",
		"@types/passport-jwt": "3.0.3",
		"@types/passport-local": "1.0.33",
		"@types/sanitize-html": "1.23.2",
		"@types/sharp": "^0.25.0",
		"@types/ua-parser-js": "0.7.33",
		"@types/uuid": "8.0.0",
		"@types/validator": "13.0.0",
		"chai": "4.2.0",
		"chai-http": "4.3.0",
		"cross-env": "7.0.2",
		"mocha": "8.0.1",
		"ts-loader": "7.0.5",
		"tsc-watch": "4.2.8",
		"tslint": "6.1.2",
		"typescript": "3.9.5",
		"webpack": "4.43.0",
		"webpack-cli": "3.3.11"
	}
}
