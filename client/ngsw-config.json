{
	"index": "/index.html",
	"assetGroups": [
		{
			"name": "app",
			"installMode": "prefetch",
			"resources": {
				"files": [
					"/favicon.ico",
					"/index.html",
					"/*.css",
					"/*.js"
				]
			}
		},
		{
			"name": "assets",
			"installMode": "lazy",
			"updateMode": "prefetch",
			"resources": {
				"files": [
					"/assets/**"
				]
			}
		},
		{
			"name": "fonts",
			"resources": {
				"urls": [
					"https://fonts.googleapis.com/**",
					"https://fonts.gstatic.com/**"
				]
			}
		}
	],
	"dataGroups": [
		{
			"name": "api-freshness",
			"urls": [
				"/api/cms$"
			],
			"cacheConfig": {
				"strategy": "freshness",
				"maxSize": 100,
				"maxAge": "1d",
				"timeout": "10s"
			}
		},
		{
			"name": "api-performance",
			"urls": [
				"/api/cms/**"
			],
			"cacheConfig": {
				"strategy": "performance",
				"maxSize": 100,
				"maxAge": "2m"
			}
		}
	]
}
