	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build": "ng build --prod",
		"build:uncommon": "ng build --prod -cc=false",
		"build:analyze": "ng build --prod -nc --stats-json -sm && webpack-bundle-analyzer dist/browser/stats.json",
		"build:server": "ng build --prod --app 1 --output-hashing=false",
		"build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
		"serve:ssr": "node dist/server.js",
		"build:client-and-server-bundles": "ng build --prod && ng build --prod --app 1 --output-hashing=false",
		"webpack:server": "webpack --config webpack.server.config.js --progress --colors",
		"test": "ng test",
		"lint": "ng lint",
		"e2e": "ng e2e",
		"analyze": "webpack-bundle-analyzer dist/browser/stats.json",
		"smexplore": "source-map-explorer",
		"analyze:build": "ng build --prod -nc --stats-json -sm && webpack-bundle-analyzer dist/browser/stats.json"
	},
