{
	"name": "sotingane",
	"version": "0.0.0",
	"license": "MIT",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build": "ng build --prod",
		"build:analyze": "npm run analyze:build",
		"build:uncommon": "ng build --prod --cc=false",
		"build:server": "ng run sotingane:server:production",
		"build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
		"serve:ssr": "node dist/server.js",
		"build:client-and-server-bundles": "ng build --prod && ng run sotingane:server:production",
		"webpack:server": "webpack --config webpack.server.config.js --progress --colors",
		"test": "ng test --browsers=ChromeHeadless --prod --watch=false",
		"lint": "ng lint",
		"e2e": "ng e2e",
		"analyze": "webpack-bundle-analyzer dist/browser/stats.json",
		"analyze:build": "ng build --prod --named-chunks --stats-json --source-map && webpack-bundle-analyzer dist/browser/stats.json"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "6.1.0",
		"@angular/cdk": "6.4.2",
		"@angular/common": "6.1.0",
		"@angular/compiler": "6.1.0",
		"@angular/core": "6.1.0",
		"@angular/forms": "6.1.0",
		"@angular/http": "6.1.0",
		"@angular/material": "6.4.2",
		"@angular/platform-browser": "6.1.0",
		"@angular/platform-browser-dynamic": "6.1.0",
		"@angular/platform-server": "6.1.0",
		"@angular/router": "6.1.0",
		"@angular/service-worker": "6.1.0",
		"@nguniversal/common": "^6.0.0",
		"@nguniversal/express-engine": "^6.0.0",
		"@nguniversal/module-map-ngfactory-loader": "^6.0.0",
		"cookie-parser": "^1.4.3",
		"core-js": "^2.5.7",
		"date-fns": "^2.0.0-alpha.14",
		"express": "^4.16.3",
		"hammerjs": "^2.0.8",
		"intersection-observer": "^0.5.0",
		"rxjs": "^6.2.2",
		"zone.js": "^0.8.26"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "0.8.0-beta.0",
		"@angular/cli": "6.2.0-beta.0",
		"@angular/compiler-cli": "6.1.0",
		"@angular/language-service": "6.1.0",
		"@types/cookie-parser": "^1.4.1",
		"@types/express": "^4.16.0",
		"@types/hammerjs": "^2.0.35",
		"@types/jasmine": "^2.8.8",
		"@types/jasminewd2": "^2.0.3",
		"@types/node": "10.5.4",
		"codelyzer": "^4.4.2",
		"jasmine-core": "^3.1.0",
		"jasmine-spec-reporter": "^4.2.1",
		"karma": "^2.0.5",
		"karma-chrome-launcher": "^2.2.0",
		"karma-cli": "^1.0.1",
		"karma-coverage-istanbul-reporter": "^2.0.1",
		"karma-jasmine": "^1.1.2",
		"karma-jasmine-html-reporter": "^1.2.0",
		"karma-scss-preprocessor": "^3.0.0",
		"protractor": "^5.4.0",
		"ts-loader": "4.4.2",
		"ts-node": "^7.0.0",
		"tslint": "^5.11.0",
		"typescript": "2.9.2",
		"webpack-bundle-analyzer": "^2.13.1",
		"webpack-cli": "^3.1.0"
	}
}
