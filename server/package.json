{
	"name": "server",
	"version": "0.0.0",
	"description": "Server",
	"license": "ISC",
	"main": "dist/index.js",
	"scripts": {
		"build": "tsc",
		"dev": "concurrently \"tsc --watch\" \"nodemon node_modules/nodemon/bin/nodemon.js --watch dist dist/index.js\"",
		"lint": "tslint node_modules/tslint/bin/linter.js -c tslint.json 'src/**/*.ts'",
		"test": "npm run compile && cross-env NODE_ENV=test mocha --exit ./dist/**/*.test.js",
		"test:debug": "npm run compile && mocha --inspect-brk --exit ./dist/**/*.test.js"
	},
	"dependencies": {
		"compression": "^1.7.1",
		"config": "^1.29.4",
		"express": "^4.16.2",
		"express-subdomain": "^1.0.5",
		"helmet": "^3.11.0",
		"https": "^1.0.0",
		"morgan": "^1.9.0",
		"spdy": "^3.4.7"
	},
	"devDependencies": {
		"@types/compression": "0.0.35",
		"@types/config": "0.0.34",
		"@types/express": "^4.11.1",
		"@types/filesystem": "0.0.28",
		"@types/helmet": "0.0.37",
		"@types/morgan": "^1.7.35",
		"@types/node": "^9.4.6",
		"@types/spdy": "^3.4.4",
		"concurrently": "^3.5.1",
		"cross-env": "^5.1.3",
		"nodemon": "^1.15.0",
		"tslint": "^5.9.1",
		"typescript": "^2.7.2"
	}
}
